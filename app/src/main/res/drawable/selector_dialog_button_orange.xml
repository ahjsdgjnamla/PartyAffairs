<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <shape xmlns:android="http://schemas.android.com/apk/res/android" android:shape="rectangle" android:useLevel="false">
            <corners android:radius="7dp" />
            <gradient android:angle="90" android:endColor="#df9a5d" android:startColor="#d65901" />
        </shape>

    </item>
    <item android:state_pressed="false">
        <shape xmlns:android="http://schemas.android.com/apk/res/android" android:shape="rectangle" android:useLevel="false">
            <gradient android:angle="90" android:endColor="#FEB06A" android:startColor="#F06400" />
            <corners android:radius="7dp" />
        </shape>
    </item>
</selector>
